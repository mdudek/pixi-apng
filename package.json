  "bugs": {
    "url": "https://github.com/mdudek/pixi-apng/issues"
  },
  "homepage": "https://github.com/mdudek/pixi-apng#readme",
  "dependencies": {
    "pako": "2.1.0",
    "pixi.js": "^8.8.1",
