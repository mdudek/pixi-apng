{
  "name": "@mdudek/pixi-apng",
  "version": "1.0.0",
  "description": "Let pixi.js support apng images",
  "main": "dist/lib/pixiApng.js",
  "types": "dist/lib/pixiApng.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "build": "rimraf ./dist && webpack --mode development --env minify=true --env library=true",
